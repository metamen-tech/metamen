{
  "name": "metamen100",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=github-actions",
    "db:generate": "supabase db diff --use-migra -f",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:seed": "supabase db seed",
    "db:reset": "supabase db reset",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next node_modules",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "knip",
    "verify": "bash scripts/verify-setup.sh",
    "inngest:dev": "inngest-cli dev",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@hookform/resolvers": "^4.1.0",
    "@supabase/ssr": "^0.6.0",
    "@supabase/supabase-js": "^2.49.0",
    "@upstash/ratelimit": "^2.0.0",
    "@upstash/redis": "^1.34.0",
    "bullmq": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^11.0.0",
    "immer": "^10.1.0",
    "inngest": "^3.0.0",
    "lucide-react": "^0.400.0",
    "next": "~15.1.0",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-hook-form": "^7.54.0",
    "recharts": "^2.15.0",
    "stripe": "^17.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/eslintrc": "^3.3.4",
    "@next/eslint-plugin-next": "^15.1.12",
    "@playwright/test": "^1.50.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "~6.6.4",
    "@testing-library/react": "~16.1.0",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "~2.1.9",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "~15.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-security": "^4.0.0",
    "fast-check": "^4.0.0",
    "husky": "^9.0.0",
    "inngest-cli": "^1.3.0",
    "jsdom": "~25.0.1",
    "knip": "^5.85.0",
    "lint-staged": "^16.2.7",
    "pg": "^8.18.0",
    "playwright": "^1.58.2",
    "prettier": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "supabase": "^2.19.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.56.1",
    "vitest": "~2.1.9"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "sharp": "^0.33.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "inngest-cli",
      "msgpackr-extract",
      "protobufjs",
      "supabase"
    ]
  }
}
